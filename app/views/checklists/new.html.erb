<h1>New Checklist</h1>

<%= form_for [@project , @checklist] do |f| %>
 <div class="panel panel-primary">
    <div class="panel-heading">
      Panel content
    </div>
    <div class="panel-footer">
      
    <div class="field">
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </div>
    <div class="field">
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
    </div>
</div>
<% end %>